<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heng.mapper.SpuViewLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.heng.domain.SpuViewLog">
        <id column="id" property="id" />
        <result column="spu_id" property="spuId" />
        <result column="spu_name" property="spuName" />
        <result column="user_id" property="userId" />
        <result column="create_time" property="createTime" />
        <result column="tenant_id" property="tenantId" />
        <result column="tenant_name" property="tenantName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, spu_id AS spuId, spu_name AS spuName, user_id AS userId, create_time AS createTime, tenant_id AS tenantId, tenant_name AS tenantName
    </sql>

</mapper>
